{
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@svgr/webpack": "^8.1.0",
    "axios": "^1.7.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "expo": "^51.0.0",
    "expo-constants": "~16.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.21",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.2",
    "next": "^14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@expo/next-adapter": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.12",
    "@types/react": "~18.2.79",
    "ignore-loader": "^0.1.2",
    "react-native-svg-transformer": "^1.5.0",
    "typescript": "~5.3.3"
  },
  "scripts": {
    "ifConfig": "ifconfig | grep inet",
    "dev": "next dev -p 3001 -H 0.0.0.0",
    "start": "next start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "next build",
    "typescript": "npx expo install -- --save-dev typescript @types/react",
    "install:core:expo": "npx expo install expo-splash-screen && npx expo install expo-router react-native-safe-area-context react-native-screens expo-linking expo-constants expo-status-bar",
    "install:core:nextjs": "npm install ignore-loader",
    "install:core": "npm run install:core:expo && npm run install:core:nextjs",
    "install:webView": "npx expo install react-native-webview",
    "install:cookie": "npx expo install @react-native-async-storage/async-storage",
    "install:svg:expo": "npx expo install react-native-svg && npx expo install react-native-svg-transformer -- --save-dev",
    "install:svg:nextjs": "npm install @svgr/webpack",
    "install:svg": "npm run install:svg:expo && npm run install:svg:nextjs",
    "install:backend": "npm install axios cors mongodb mongoose bcryptjs jsonwebtoken && npm install --save-dev @types/bcryptjs @types/jsonwebtoken",
    "install:all": "npm run install:core && npm run install:webView && npm run install:cookie && npm run install:svg && npm run install:backend"
  },
  "name": "ai-chatbot-yoo-v2",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true
}
