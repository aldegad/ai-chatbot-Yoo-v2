  "scripts": {
    "ifConfig": "ifconfig | grep inet",
    "dev": "next dev -p 3001 -H 0.0.0.0",
    "start": "next start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "next build",
    "moduleDefault:expo": "npx expo install expo-splash-screen && npx expo install expo-router react-native-safe-area-context react-native-screens expo-linking expo-constants expo-status-bar",
    "moduleDefault:nextjs": "npm install ignore-loader",
    "moduleAddon:webView": "npx expo install react-native-webview",
    "moduleAddon:cookie": "npx expo install @react-native-async-storage/async-storage",
    "svg:expo": "npx expo install react-native-svg && npx expo install react-native-svg-transformer -- --save-dev",
    "svg:nextjs": "npm install @svgr/webpack",
    "axios-mongodb-jwt": "npm install axios cors mongodb mongoose bcryptjs jsonwebtoken && npm install --save-dev @types/bcryptjs @types/jsonwebtoken",
    "dependencies": "npm run moduleDefault:expo && npm run moduleDefault:nextjs && npm run moduleAddon:webView && npm run moduleAddon:cookie && npm run svg:expo && npm run svg:nextjs && npm run axios-mongodb-jwt"
  },
